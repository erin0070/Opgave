<script>

    var bgpic = document.getElementsByClassName("backgroundpicture")[0];

    for (i = 0; i < backgroundpicture.length; i++) {
    
        backgroundpicture[i].addEventListener("hoverOver", manageHoverEffects);
        backgroundpicture[i].addEventListener("hoverOFF", manageHoverEffects);
    }
    
    function manageHoverEffects() {
        
        if 
            (this.getAttribute("hoverOver")){
                bgpic.classList.add(this.getAttribute("hoverOver"));
        }

        if 
            (this.getAttribute("hoverOFF")){
                bgpic.classList.remove(this.getAttribute("hoverOFF"));
        }
    }
    
   
);

</script>




































